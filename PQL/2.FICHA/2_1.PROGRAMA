ACCEPT NUMERO PROMPT 'ESCRIBE UN NUMERO: ';
DECLARE
    NUMERO NUMBER(9) := &NUMERO;
    ESPRIMO NUMBER(9):=0;
	CONT NUMBER(2):=1;

BEGIN
	
	WHILE CONT<=NUMERO LOOP	
		
	IF NUMERO MOD CONT=0 THEN
		ESPRIMO :=ESPRIMO +1;
	
	END IF;
	CONT:=CONT+1;			

	END LOOP;
	
	IF ESPRIMO =2 THEN
		 DBMS_OUTPUT.PUT_LINE(NUMERO|| ' ES PRIMO ' );
	ELSE 
		 DBMS_OUTPUT.PUT_LINE(NUMERO|| ' NO ES PRIMO ' );
	END IF;


END;
/

