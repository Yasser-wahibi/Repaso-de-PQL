ACCEPT NOTA PROMPT 'ESCRIBE UN NUMERO: '


DECLARE
  NUM1 NUMBER(5) := &NOTA;

BEGIN
  IF NUM1 < 5 THEN 
    DBMS_OUTPUT.PUT_LINE('HAS SACADO INSIFICIENTE');
  ELSIF NUM1 >=5 AND NUM1<=6 THEN
     DBMS_OUTPUT.PUT_LINE('HAS SACADO SUFICIENTE');
 ELSIF NUM1 >=6 AND NUM1<=7 THEN
     DBMS_OUTPUT.PUT_LINE('HAS SACADO BIEN');
 ELSIF NUM1 >=7 AND NUM1<=9 THEN
     DBMS_OUTPUT.PUT_LINE('HAS SACADO NOTABLE');
  ELSE
     DBMS_OUTPUT.PUT_LINE('HAS SACADO SOBRESALIENTE');

  END IF;
END;
/
